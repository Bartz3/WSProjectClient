{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "toastr@2.1.4",
      "destination": "wwwroot/lib/toastr/",
      "files": [
        "build/toastr.css",
        "build/toastr.js.map",
        "build/toastr.min.css",
        "build/toastr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.js",
        "jquery.min.js",
        "jquery.min.map",
        "jquery.slim.js",
        "jquery.slim.min.js",
        "jquery.slim.min.map"
      ]
    }
  ]
}